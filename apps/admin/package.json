{
  "name": "@nicolatetcholdiwsconsole/admin",
  "version": "2.0.0",
  "description": "Vue 3 + TypeScript admin dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@type/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.0",
    "vue-tsc": "^1.8.13"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
